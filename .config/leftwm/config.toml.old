modifier = ["modkey"]
key = "f"

[[keybind]]
command = "Execute"
value = "pamixer --allow-boost -i 5"
modifier = []
key = "XF86AudioRaiseVolume"

[[keybind]]
command = "Execute"
value = "pamixer --allow-boost -d 5"
modifier = []
key = "XF86AudioLowerVolume"

[[keybind]]
command = "Execute"
value = "pamixer -t"
modifier = []
key = "XF86AudioMute"

[[keybind]]
command = "Execute"
value = "playerctl play-pause"
